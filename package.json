{
  "name": "package",
  "version": "0.0.0",
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "build:perceive-east": "vue-tsc --noEmit && vite build --mode prod_percive_east",
    "build:perceive-west": "vue-tsc --noEmit && vite build --mode prod_percive_west",
    "build:police-east": "vue-tsc --noEmit && vite build --mode prod_police_east",
    "build:police-west": "vue-tsc --noEmit && vite build --mode prod_police_west",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "prettier": "prettier --write .",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "element-plus": "^1.3.0-beta.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.9",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.1",
    "mrm": "^3.0.10",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "sass": "^1.47.0",
    "tailwindcss": "^3.0.13",
    "typescript": "^4.4.4",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.17.11",
    "vite": "^2.7.2",
    "vite-plugin-compression": "^0.4.0",
    "vite-plugin-svg-icons": "^1.1.0",
    "vue-tsc": "^0.29.8"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "prettier --write ./src",
      "eslint --ext .vue,.js,.jsx --fix ./src",
      "git add"
    ],
    "*.{ts,tsx,vue,js,jsx}": "eslint --cache --fix"
  }
}
